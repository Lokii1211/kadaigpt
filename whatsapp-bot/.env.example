# KadaiGPT WhatsApp Bot Configuration
# Copy this file to .env and fill in the values

# Backend API URL
BACKEND_URL=http://localhost:8000/api/v1

# Store ID (get from your KadaiGPT admin)
STORE_ID=1

# Owner's phone number for startup notifications (with country code, no +)
# Example: 919876543210
OWNER_NUMBER=

# Staff members to receive daily summaries
STAFF_NUMBERS=

# Enable debug mode
DEBUG=false
